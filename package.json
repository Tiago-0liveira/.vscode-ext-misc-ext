{
	"name": "misc-ext",
	"displayName": "misc-ext",
	"description": "miscelaneous - commands/ easy and usefull options over here and there",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/Tiago-0liveira/.vscode-ext-misc-ext.git"
	},
	"icon": "icon.png",
	"engines": {
		"vscode": "^1.49.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": []
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
